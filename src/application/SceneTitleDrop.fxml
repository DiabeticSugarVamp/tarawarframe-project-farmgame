   <bottom>
      <Label onMouseClicked="#switchToSceneIntro" styleClass="button-cancel" text="PROCEED" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="20.0" />
         </BorderPane.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </bottom>
</BorderPane>
